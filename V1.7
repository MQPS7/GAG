local function NewFrame(FrameType, Position, Size, Zindex, Parent, BGC, In1, TorF, In2)-----Huc = Have Ui Conner  ,  BGC = BackgroundColor3  , TorF = Ture Or False
	if TorF == nil then TorF = true end
	local ReturItem = nil
	if FrameType == "Frame" then
		local Item = Instance.new(FrameType);		local UICornerFrame1 = Instance.new("UICorner")
		Item.Parent = Parent;		Item.Position = Position
		Item.Size = Size;		Item.BackgroundColor3 = BGC
		Item.ZIndex = Zindex;		Item.Visible = TorF
		UICornerFrame1.Parent = Item;		ReturItem = Item
	elseif FrameType == "TextLabel" then----In1 = text
		local Item = Instance.new("TextLabel")
		Item.Parent = Parent;		Item.Position = Position
		Item.Size = Size;		Item.BackgroundTransparency = 1
		Item.Font = Enum.Font.Highway;		Item.TextScaled = true
		Item.ZIndex = Zindex;		Item.RichText = true
		if In2 == "Left" then Item.TextXAlignment = Enum.TextXAlignment.Left end
		Item.Text = In1;		Item.TextColor3 = BGC;		ReturItem = Item
	elseif FrameType == "ImageLabel" then----In1 = ImageId
		local Item = Instance.new(FrameType);		Item.Image = In1
		Item.Parent = Parent;		Item.Position = Position
		Item.Size = Size;		Item.BackgroundTransparency = 1
		Item.ZIndex = Zindex;		Item.ImageColor3 = BGC
		ReturItem = Item;
	end
	return ReturItem
end
