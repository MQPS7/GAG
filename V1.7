local function createSlider(parent, position, size, minValue, maxValue, defaultValue, labelText)
	local mainLine = Instance.new("Frame");	local line = Instance.new("Frame")
	local lineButton = Instance.new("Frame");	local textLabel = Instance.new("TextLabel")
	local uiCornerMain = Instance.new("UICorner");	local uiCornerLine = Instance.new("UICorner")
	local uiCornerButton = Instance.new("UICorner")
	mainLine.Parent = parent;	mainLine.Position = position
	mainLine.Size = size;	mainLine.BackgroundColor3 = Color3.new(0.176471, 0.121569, 0.294118)
	mainLine.BorderSizePixel = 0;	uiCornerMain.Parent = mainLine
	line.Parent = mainLine;	line.Position = UDim2.new(0, 0, 0, 0)
	line.Size = UDim2.new(1, 0, 1, 0);	line.BackgroundColor3 = Color3.new(0.517647, 0.329412, 1)
	line.BorderSizePixel = 0;	uiCornerLine.Parent = line
	lineButton.Parent = mainLine;	lineButton.Position = UDim2.new(0.9, 0, -0.1, 0)
	lineButton.Size = UDim2.new(0.1, 0, 1.2, 0);	lineButton.BackgroundColor3 = Color3.new(0.45098, 0, 1)
	lineButton.BorderSizePixel = 0;	uiCornerButton.Parent = lineButton
	textLabel.Parent = mainLine;	textLabel.Position = UDim2.new(0, 0, -1.5, 0)
	textLabel.Size = UDim2.new(1, 0, 1, 0);	textLabel.Text = "<b> " .. labelText .. " </b>"
	textLabel.TextScaled = true;	textLabel.RichText = true; textLabel.TextXAlignment = Enum.TextXAlignment.Left
	textLabel.BackgroundTransparency = 1;	textLabel.TextColor3 = Color3.new(0.407843, 0.352941, 1)	
	local value = defaultValue
	local function updateSlider(inputX)
		local sliderSize = mainLine.AbsoluteSize.X;		local sliderPos = mainLine.AbsolutePosition.X
		local relativeX = math.clamp(inputX - sliderPos, 0, sliderSize);		local percent = relativeX / sliderSize
		value = minValue + (maxValue - minValue) * percent;		line.Size = UDim2.new(percent, 0, 1, 0)
		lineButton.Position = UDim2.new(percent, -lineButton.AbsoluteSize.X / 2, 0.5, -lineButton.AbsoluteSize.Y / 2)
		textLabel.Text = "<b> " .. labelText .. " " .. math.floor(value) .. " </b>"

		if labelText == "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏≤‡∏£‡πÄ‡∏î‡∏¥‡∏ô" then
			WalkSpeedValue = value
		end		
	end

	local function onInputBegan(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			local moveConnection, releaseConnection
			moveConnection = UIS.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
					updateSlider(input.Position.X)
					if input.UserInputType == Enum.UserInputType.Touch then
						Camera.CameraType = Enum.CameraType.Scriptable
					end
				end
			end)
			releaseConnection = UIS.InputEnded:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
					if moveConnection then moveConnection:Disconnect() end
					if releaseConnection then releaseConnection:Disconnect() end
					Camera.CameraType = Enum.CameraType.Custom
				end
			end)
		end
	end
	lineButton.InputBegan:Connect(onInputBegan)
	return mainLine
end
