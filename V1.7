local allowedKeywords = {
	"Strawberry",
	"Carrot",
	"Blueberry",
	"Tomato",
	"Raspberry",
	"Cranberry"
}
local function matchesAllowedKeyword(value)
	value = value:lower()
	for _, keyword in ipairs(allowedKeywords) do
		if string.find(value, keyword:lower()) then
			return true
		end
	end
	return false
end
local function equipToolWithItemStringMatch(player)
	local backpack = player:FindFirstChild("Backpack")
	local character = player.Character
	if not backpack or not character then return "NotFound" end
	local allTools = {}
	for _, tool in ipairs(backpack:GetChildren()) do
		if tool:IsA("Tool") then
			table.insert(allTools, tool)
		end
	end
	for _, tool in ipairs(character:GetChildren()) do
		if tool:IsA("Tool") then
			table.insert(allTools, tool)
		end
	end
	for _, tool in ipairs(allTools) do
		local itemString = tool:FindFirstChild("Item_String")
		if itemString and itemString:IsA("StringValue") then
			if matchesAllowedKeyword(itemString.Value) then
				tool.Parent = character
				return true
			end
		end
	end
	return "NotFound"
end
local function GetnearestPet()
	local nearestPet = nil
	local minDistance = math.huge
	local playerPosition = HMNRT.Position
	local function GetNearFungtion(EnamyModel)
		if EnamyModel:IsA("Model") and EnamyModel.PrimaryPart then
			local PetAreaPosition = PetArea.Position
			local RootPart = EnamyModel.PrimaryPart
			local distance = (PetAreaPosition - RootPart.Position).Magnitude
			if distance < minDistance then
				if distance <= 70 then
					if distance < minDistance then
						minDistance = distance
						nearestPet = EnamyModel
					end
				end
			end
		end
	end
	local PetsPhysical = workspace:FindFirstChild("PetsPhysical")
	for _, Pet in ipairs(PetsPhysical:GetChildren()) do
		if Pet:FindFirstChildOfClass("Model") then
			local EnamyModel = Pet:FindFirstChildOfClass("Model")
			if EnamyModel:IsA("Model") and EnamyModel.PrimaryPart then
				GetNearFungtion(EnamyModel)
			end
		end
	end
	return nearestPet
end

